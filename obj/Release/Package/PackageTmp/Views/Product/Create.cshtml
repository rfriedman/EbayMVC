@model IEnumerable<EbayMVC.Models.Products>

@{
    ViewBag.Title = "Search Results";
}

<h2>Create</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>


    <div id="container">
@foreach (var item in Model)
{
    

        

	<div class="grid">
		<div class="imgholder">
			<img src=" @Html.DisplayFor(modelItem => item.gallery_url)" />
		</div>
		<strong>@Html.DisplayFor(modelItem => item.item_title)</strong>
		<p><a href=" @Html.DisplayFor(modelItem => item.listing_url)"> Click to view</a></p>
</div>
}
</div>  

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
